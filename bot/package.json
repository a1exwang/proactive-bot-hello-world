{
  "name": "echobot",
  "version": "1.0.0",
  "description": "Microsoft Teams Toolkit hello world Bot sample",
  "author": "Microsoft",
  "license": "MIT",
  "scripts": {
    "dev:teamsfx": "env-cmd --silent -f .env.teamsfx.local npm run dev",
    "dev": "func start --typescript --language-worker=\"--inspect=9239\" --port \"3978\" --cors \"*\"",
    "watch:teamsfx": "tsc --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "npx func start",
    "dev:teamsfx:prepare-storage": "azurite --silent --location ./_storage_emulator --debug ./_storage_emulator/debug.log"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "dependencies": {
    "botbuilder": "~4.14.0"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.3",
    "azurite": "^3.15.0",
    "env-cmd": "^10.1.0",
    "ts-node": "~9.1.1",
    "typescript": "~3.9.2"
  }
}
